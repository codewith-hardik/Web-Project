<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>All Users</title>
    <style>
        table, td, th{
            border: 1px solid black;
        }
    </style>
</head>
<body>
    <table>
        <tr>
            <th>Id</th>
            <th>Email</th>
            <th>UserName</th>
            <th>Edit</th>
            <th>Delete</th>
        </tr>

        <%  for( user of users){   %>
            <tr>

                <td><%= user.id %></td>
                <td><%= user.email %></td>
                <td><%= user.username %></td>
               <form action="/users/<%= user.id %>/edit" method="get">
                    <td><button>Edit UserName</button></td>
               </form>
               <form action="/users/<%= user.id %>/delete" method="get">
                    <td><button> Delete User</button></td>
               </form>
            </tr>
         <% } %>
        
    </table>
    
</body>
</html>


